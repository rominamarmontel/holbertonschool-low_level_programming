#include <stdio.h>

/*
 * 6. Size is not grandeur, and territory does not make a nation
 */ int main(void)
{
printf("Size of a char: %d byte(s)\n");
printf("Size of an int: %lu  byte(s)\n");
printf("Size of a long int: %ld byte(s)\n");
printf("Size of a long long int:  byte(s)\n");
printf("Size of a float:  byte(s)\n");
return (0);
}
